package ch17;
import java.util.Scanner;

public class Ch17 {

    public static void main(String[] args) {
        
        Scanner sc = new Scanner (System.in);
        
        int n1 = 0;
        int n2 = 1;
        int n3;
        System.out.print("Number of Fibonacci terms: ");
        int count = sc.nextInt();
        
        for (int i = 0; i < count; i++) {
            n3 = n1 + n2;
            System.out.print(" "+n3);
            n1 = n2;
            n2 = n3;
        }
    }
}
